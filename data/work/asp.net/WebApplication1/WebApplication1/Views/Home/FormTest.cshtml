
<h2>FormTest</h2>
<form method="post" action="/Home/FormTest">
    <p><input type="checkbox" name="bike" id="bike" value="Bike" /> 自行车</p>
    <p><input type="checkbox" name="car" id="car" value="Car" checked="checked" /> 汽车</p>
    <select id="week" name="week">
        @foreach (WebApplication1.Models.Week week in ViewBag.dic)
        {
        <option value=@week.Number>@week.Name</option>
        }
    </select>
    <input type="submit" value="提交" />
</form>