<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#0000ff"
    android:layout_margin="10sp"
    android:layout_height="match_parent">
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:background="#ffffffff"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="详细信息"
            android:textSize="18sp"
            android:textColor="#0000ff"
            android:background="#ffffffff"
            android:gravity="center"
            />
    </TableRow>
<TableRow
    android:layout_margin="1dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="快递类型"
        android:layout_weight="1"
        android:background="#ffffffff"
        android:textColor="#000000"
        android:textSize="16sp"
        />
    <TextView
        android:layout_marginLeft="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/type"
        android:layout_weight="1"
        android:background="#ffffffff"
        android:textSize="16sp"
        android:textColor="#000000"
        />
</TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"

        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="业务分类"
            android:background="#ffffffff"
            android:layout_weight="1"
            android:textColor="#000000"
            android:textSize="16sp"
            />
        <TextView
            android:layout_marginLeft="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:id="@+id/classify"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="业务类型"
            android:background="#ffffffff"
            android:textColor="#000000"
            android:layout_weight="1"
            android:textSize="16sp"
            />
        <TextView
            android:layout_marginLeft="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="韵达快递"
            android:background="#ffffffff"
            android:id="@+id/reason"
            android:layout_weight="1"
            android:textSize="16sp"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账单时间"
            android:background="#ffffffff"
            android:layout_weight="1"
            android:textColor="#000000"
            android:textSize="16sp"
            />
        <TextView
            android:layout_marginLeft="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账单时间"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:id="@+id/billingTime"
            android:textSize="16sp"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="金额"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:textColor="#000000"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="韵达快递"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:id="@+id/price"
            android:layout_marginLeft="2dp"
            android:textSize="16sp"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="客户"
            android:background="#ffffffff"
            android:textColor="#000000"
            android:layout_weight="1"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="韵达快递"
            android:background="#ffffffff"
            android:layout_weight="1"
            android:textSize="16sp"
            android:layout_marginLeft="2dp"
            android:id="@+id/guest"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="备注"
            android:background="#ffffffff"
            android:textColor="#000000"
            android:layout_weight="1"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="韵达快递"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:textSize="16sp"
            android:id="@+id/remark"
            android:layout_marginLeft="2dp"
            android:textColor="#000000"
            />

    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="用户名"
            android:background="#ffffffff"
            android:layout_weight="1"
            android:textColor="#000000"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="创建人"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:id="@+id/createuser"
            android:textSize="16sp"
            android:layout_marginLeft="2dp"
            android:textColor="#000000"
            />
    </TableRow>
    <TableRow
        android:layout_margin="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="创建时间"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:textColor="#000000"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="创建时间"
            android:background="#ffffffff"
            android:layout_weight="1"
            android:id="@+id/createTime"
            android:textSize="16sp"
            android:layout_marginLeft="2dp"
            android:textColor="#000000"
            />

    </TableRow>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="返回"
        android:id="@+id/back"
        android:background="#EE6AA7"
        android:textColor="#ffffff"
        />
</TableLayout>
