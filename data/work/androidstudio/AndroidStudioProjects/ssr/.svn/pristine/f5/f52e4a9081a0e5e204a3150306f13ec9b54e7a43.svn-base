<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:background="@drawable/linearlayout_shape_corner"
                android:orientation="vertical"
                >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#686867" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_shape_corner_top"
                    android:gravity="center"
                    android:text="项目维护详细"
                    android:textColor="#0e2d5f"
                    android:textSize="18sp" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#686867" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="项目名称"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3" />
                    <TextView
                        android:id="@+id/project_name"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="项目经理"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3" />
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/project_manager"
                        android:textColor="#000000"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="预计开始时间"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3" />
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/yjstart_time"
                        android:textColor="#000000"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="实际开始时间"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3" />
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/sjstart_time"
                        android:textColor="#000000"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="项目描述"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3" />
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/states"
                        android:textColor="#000000"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <View
                    android:layout_marginTop="1dp"
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/linearlayout_shape_corner_center"
                    >
                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingBottom="10dip"
                        android:paddingTop="10dip"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:text="序号" />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>
                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="周期" />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>
                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dip"
                        android:paddingTop="10dip"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="回款" />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>
                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dip"
                        android:textColor="#000000"
                        android:paddingTop="10dip"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="描述" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#A7A5A5"/>

                <ListView
                    android:layout_weight="1"
                    android:id="@+id/lvs"
                    android:divider="#B4B3B3"
                    android:dividerHeight="1.0dp"
                    android:scrollbars="none"
                    android:cacheColorHint="@android:color/transparent"
                    android:fadingEdge="none"
                    android:layout_width="match_parent"
                    android:listSelector="#ffff00"
                    android:layout_height="match_parent" >
                </ListView>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dip"
                    android:background="#B4B3B3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/linearlayout_shape_corner_center"
                    >
                    <TextView
                        android:id="@+id/text_id"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingBottom="10dip"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:text="序号" />

                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="项目成员" />
                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>
                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dip"
                        android:paddingTop="10dip"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="成员加入时间" />

                    <View
                        android:layout_width="1.5dip"
                        android:layout_height="fill_parent"
                        android:background="#B4B3B3"/>

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:paddingBottom="10dip"
                        android:textColor="#000000"
                        android:paddingTop="10dip"
                        android:textSize="15sp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:text="成员离开时间" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#A7A5A5"/>
                <ListView
                    android:layout_weight="1"
                    android:id="@+id/lv"
                    android:divider="#B4B3B3"
                    android:dividerHeight="1.0dp"
                    android:scrollbars="none"
                    android:cacheColorHint="@android:color/transparent"
                    android:fadingEdge="none"
                    android:layout_width="match_parent"
                    android:listSelector="#ffff00"
                    android:layout_height="match_parent" >
                </ListView>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>




</LinearLayout>
